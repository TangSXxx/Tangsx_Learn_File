                <property name="styleSheet">
                 <string notr="true">QTabWidget::pane {
    border: 1px solid #C0D8DA;
    background-color: white;
    border-radius: 6px;
    margin: 0px;
    padding: 0px;
}

/* 标签页容器 */
QTabWidget::tab-bar {
    alignment: left;
    margin: 0px;
    padding: 0px;
}

/* 单个标签页 - 未选中状态 */
QTabBar::tab {
    background-color: white;
    border: 1px solid #C0D8DA;
    border-bottom: none;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    min-width: 130px;
    padding: 10px 18px;
    margin-right: 3px;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;, sans-serif;
    font-size: 13px;
    font-weight: normal;
    color: #666666;
}

/* 选中的标签页 - 使用淡蓝色背景并增大字体 */
QTabBar::tab:selected {
    background-color: rgb(90,194,198);
    border-color: #C0D8DA;
    border-bottom-color: rgb(90,194,198);
    font-weight: 600;
    font-size: 14px;
    color: white;
}

/* 未选中的标签页悬停效果 */
QTabBar::tab:!selected:hover {
    background-color: #F0F8F8;
    color: #333333;
}

/* 选中的标签页悬停效果 */
QTabBar::tab:selected:hover {
    background-color: rgb(100,204,208);
    color: white;
}

/* 标签页禁用状态 */
QTabBar::tab:disabled {
    background-color: #F8F8F8;
    color: #AAAAAA;
}

/* 标签页关闭按钮样式（如果使用可关闭的标签页） */
QTabBar::close-button {
    subcontrol-origin: padding;
    subcontrol-position: right;
    width: 16px;
    height: 16px;
    background: transparent;
    border-radius: 2px;
    margin: 2px;
}

QTabBar::close-button:hover {
    background: rgba(255,255,255,0.3);
}

QTabBar::close-button:pressed {
    background: rgba(255,255,255,0.5);
}

/* 添加一些额外的美化 */
QTabBar::tab:first {
    margin-left: 5px; /* 第一个标签页左边距 */
}

QTabBar::tab:last {
    margin-right: 5px; /* 最后一个标签页右边距 */
}

/* 标签页内容区域的额外内边距 */
QTabWidget::pane &gt; QWidget {
    padding: 15px;
}
